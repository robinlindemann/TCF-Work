<aura:component controller="RecaptchaCtrl" implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="reCaptchaMessage" type="String" />
    <aura:attribute name="showNext" type="Boolean" default="false" />
    <aura:attribute name="isOpen" type="boolean" default="false"/>
    <aura:attribute name="appRecType" type="String"/>
    <aura:attribute name="newApp" type="DCE_Application__c" default="{'sobjectType':'DCE_Application__c'}"/>
    <aura:attribute name="password1" type="String"/>
	<aura:attribute name="password2" type="String"/>
        
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:registerEvent name="newCreditAppEvent" type="c:DCE_NewCreditAppEvent"/>
    
       <div class="steps">
        <div class="step-content ">
            <!--BEGIN PAGE HEADER/SUBHEADER SECTION-->
            <div class="step-header-section slds-align_absolute-center" style="width:500px">
                <h1> Let's Get Started! </h1></div>
             <div class="step-header-section slds-align_absolute-center" style="width:600px">
                <h2> Please provide primary contact information below and create a password that will allow you to re-visit 
                    this application if needed prior to final submission.  This information will also be used to contact
                    you throughout the application process with any questions.</h2>
                 <br/>
            </div>
            
            <!--END PAGE HEADER/SUBHEADER SECTION-->
            
            <!--BEGIN INPUT FIELD FORM SECTION - FIRST NAME, LAST NAME, EMAIL, PHONE, PASSWORD & VERIFY PASSWORD-->
            <div class="step-content-section slds-align_absolute-center" style="width:304px">
                
                <form class="slds-form--stacked">
                    <div class="slds-form-element__control" style="width:304px">
                      <lightning:input name="firstName" label="First Name"
                                         aura:id="fname"
                                         value="{!v.newApp.Contact_First_Name__c}"
                                         maxlength="40"
                                         required="true"
                                         messageWhenValueMissing="Please enter First Name"/>
                                         
                    <lightning:input name="lastName" label="Last Name"
                                         aura:id="lname"
                                         value="{!v.newApp.Contact_Last_Name__c}"
                                         maxlength="40"
                                         required="true"
                                     	 messageWhenValueMissing="Please enter Last Name"/>
                   <lightning:input name="email" label="Email"
                                         aura:id="em"
                                         type="email"
                                         value="{!v.newApp.Contact_Email_Address__c}"
                                         required="true"
                                   		 messageWhenTypeMismatch="Please enter valid email address"
                                    	 messageWhenValueMissing="Please enter valid email address"/>
                   <span onkeypress="{!c.enforceNumeric}">
                   <lightning:input name="tel" label="Phone"
                                             aura:id="phone"
                                             type="tel"
                                             value="{!v.newApp.Contact_Phone__c}"
                                             pattern="((\(\d{3}\) ?)|(\d{3}-))?\d{3}-\d{4}" 
                                             minlength="10" 
                                             maxlength="10" 
                                             onfocus="{!c.removeDashes}" 
                                             onblur="{!c.formatPhone}"
                                             required="true"
                                    		 messageWhenValueMissing="Please enter a valid Phone Number (10 digits no dashes)"
                                    		 messageWhenPatternMismatch="Please enter a valid Phone Number (10 digits no dashes)"/>
                      </span>
                      <lightning:input name="pw" label="Password"
                                         aura:id="pwd1"
                                         type="password"
                                         pattern="((?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[\W]).{8,100})"
                                         value="{!v.password1}"
                                         required="true"
                                       	 messageWhenValueMissing="Please enter a password with at least 1 uppercase letter, 1 lowercase letter,
                                                                     1 number, one special character and must be at least 8 characters"
                                         messageWhenPatternMismatch="Please update password to include at least 1 uppercase letter, 1 lowercase letter,
                                                                     1 number, one special character and must be at least 8 characters"/>
                     <lightning:input name="cPW" label="Confirm Password"
                                         aura:id="pwd2"
                                         type="password"
                                         pattern="((?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[\W]).{8,100})"
                                         value="{!v.password2}"
                                         required="true"
                                      	 messageWhenValueMissing="Please enter a password with at least 1 uppercase letter, 1 lowercase letter,
                                                                     1 number, one special character and must be at least 8 characters"
                                         messageWhenPatternMismatch="Please update password to include at least 1 uppercase letter, 1 lowercase letter,
                                                                     1 number, one special character and must be at least 8 characters"/>
                   </div>
                   <!--END INPUT FIELD FORM SECTION-->
                    <br/>
                    <!--BEGIN IFRAME/RECAPTCHA WIDGET SECTION -->
                    <div class="step-content-section slds-align_absolute-center">
                        <div class="slds-form-element__control" style="width: 100%;" align="center" >
                            <iframe aura:id="vfFrame" src="https://dev-tcfef.cs20.force.com/CreditApp/DCE_ReCaptchaVF" scrolling="no" frameborder="0"
                                    allowtransparency="true" width="100%" height="500px"  />
                        </div>
                    </div>
                    <!--END RECAPTCHA WIDGET SECTION -->
                    
                    <!--BEGIN SITE TERMS OF USE AND NEXT SECTION -->
                    <div class="step-content-section">
                        <aura:if isTrue="{!v.showNext}">
                            <!--SPLIT CHECKBOX COMPONENT INTO TWO SECTIONS (BOX & TEXT) TO ACCOMODATE TEXT LINK/ACTION FOR POPUP-->
                            
                            <div align="center" >
                                <lightning:layoutItem  >
                                    <lightning:input aura:id="termsBox" type="checkbox" checked="{!v.newApp.Site_Terms_of_Use_Accepted__c}" required="true" 
                                                     label="I understand that I am electronically agreeing to the Site Terms of Use for this application."  messageWhenValueMissing="Please accept terms of use"  />
                                </lightning:layoutItem></div>
                           
                            <div align="center" >
                                <h4>Click <lightning:button variant="base" label=" here " onclick="{! c.openModal }"/> to view full site terms of use.
                                </h4>
                           </div>
                            <br/>
                           <!--NEXT BUTTON-->
                            <div align="center" >
                                <h6><lightning:button label="Next" variant="brand" title="I electronically agree to the terms of use for this application." onclick="{!c.handleNextClick}"/>
                                    </h6>
                            </div>
                        </aura:if>
                    </div>
                    <!--END SITE TERMS OF USE AND NEXT SECTION -->
                </form>  
            </div>
        </div>
        
        
    </div>
    <!--BEGIN POP-UP MODAL-->
    <div class="slds-m-around_xx-large">
    <!--Use aura:if tag to open Modal, when text link is clicked -->   
        <aura:if isTrue="{!v.isOpen}">
            
            <!--###### MODAL BOX Start######--> 
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <!-- ###### MODAL BOX HEADER Start ######-->
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close"
                                              onclick="{! c.closeModal }"
                                              alternativeText="close"
                                              variant="bare-inverse"
                                              class="slds-modal__close"/>
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Terms of Use – Effective 3-29-2019</h2>
                    </header>
                    <!--###### MODAL BOX BODY Part Start######-->
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <p>To access TCF’s Wholesale Digital Services, you must first agree to these Terms and Conditions of Use (the "Terms") by following the on-screen 
